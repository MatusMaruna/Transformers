	println(x);
	println(f[3-1]);
	println(f);
	int res = sumUpTo(n);
	println(res);    # 55
	float[] ff = {9.3,8.8,17.5,1.0,2.1,3.2,4.4};
    println(ff[0]);   # 9.3
